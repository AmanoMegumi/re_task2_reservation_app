<div class="container-field user">
  <div class="row">
    <div class="col-md-12 user-view">
      <div class="user-view-box">
        <div class="update-content">
          <div class="row">
            <label class="col-md-3" for="subscribe">予約した部屋</label>
            <div class="col-md-9">
              <%= link_to @subscribe.roomname, @subscribe %>
            </div>
          </div>
        </div>

        <div class="update-content">
          <div class="row">
            <label class="col-md-3" for="start_date">開始日</label>
            <div class="col-md-9">
              <%= @reserve.start_date.to_s(:date) %>
            </div>
          </div>
        </div>

        <div class="update-content">
          <div class="row">
            <label class="col-md-3" for="end_date">終了日</label>
            <div class="col-md-9">
              <%= @reserve.end_date.to_s(:date) %>
            </div>
          </div>
        </div>

        <div class="update-content">
          <div class="row">
            <label class="col-md-3">価格</label>
            <div class="col-md-9">
              <%= @reserve.reserve_price %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>